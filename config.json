    "saveCompositeEntity": {
        "id": "instance_id",
        "entity_field": "parent_entity_id",
        "include_entities": ["layout", "fa_acl", "app_action", "fa_role", "rule_set", "catalog", "catalog_type", "rule_action", "form_rule"],
